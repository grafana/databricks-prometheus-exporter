                  "expr": "max(quantile_over_time(0.95, databricks_queries_running{job=~\"$job\",workspace_id=~\"$workspace_id\",instance=~\"$instance\"}[7d]))",
                  "format": "time_series",
                  "instant": false,
                  "legendFormat": "7d baseline",
